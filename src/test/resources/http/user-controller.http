POST http://localhost:8080/api/users/register
Content-Type: application/json

{
  "username": "testuser",
  "password": "123456",
  "email": "newuser@example.com",
  "firstName": "New",
  "lastName": "User",
  "phoneNumber": "1234567890",
  "role": "USER"
}

